@model tillidPortal.Models.Contact
<table width="900">
    <tr>
        <td style="padding: 10px; color: #333;">
            <p style="font-size: 18px; font-family: Calibri,sans-serif; font-size: 11pt; margin-top: 0px;">Hi <strong></strong>,</p>
            <p style="font-size: 18px; font-family: Calibri,sans-serif; font-size: 11pt; margin-top: 0px;">The following person have reached you.</p>
            <p style="font-size: 18px; font-family: Calibri,sans-serif; font-size: 11pt; margin-top: 0px;"><strong></strong></p>
            <table width="400px">
                <tr>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt; padding-bottom: 10px; width: 150px;"><strong>Email:</strong></td>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt;padding-bottom: 10px;">@Model.Email</td>
                </tr>
                <tr>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt; padding-bottom: 10px; width: 150px;"><strong>Country:</strong></td>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt;padding-bottom: 10px;">@Model.Country</td>
                </tr>
                <tr>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt; padding-bottom: 10px; width: 150px;"><strong>Phone:</strong></td>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt;padding-bottom: 10px;">@Model.Phone</td>
                </tr>
                <tr>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt; padding-bottom: 10px; width: 150px;"><strong>Message:</strong></td>
                    <td style="font-family: Calibri,sans-serif; font-size: 11pt;padding-bottom: 10px;">@Model.Message</td>
                </tr>
            </table>
            <table width="100%">
                <tr>
                    <td><p style="margin-bottom: 7px;margin-top: 35px; font-family: Calibri,sans-serif;">Thank You,</p><p style="margin-top: 5px; font-family: Calibri,sans-serif;">Wexos</p><p style="margin-top: 5px; padding-left: 0px;"><img src="" width="150" /></p></td>
                </tr>
            </table>
    </tr>
</table>
